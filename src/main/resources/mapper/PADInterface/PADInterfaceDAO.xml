<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tbl.modules.PADInterface.dao.PADInterfaceDAO">
    <!--获取叉车操作类型-->
    <select id="getOperateType" resultType="java.lang.String">
        select operate_type
        from slab_operate_type
        where user_ip=#{ip}
    </select>
</mapper>
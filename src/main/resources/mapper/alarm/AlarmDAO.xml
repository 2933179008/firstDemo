<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tbl.modules.alarm.dao.AlarmDAO">

    <select id="getOneTimeout" resultType="com.tbl.modules.alarm.entity.Alarm">
        select a.* from alarm a where a.type= 1 and a.bind_code = #{bindcode}
    </select>

    <select id="getOneQualityTimeout" resultType="com.tbl.modules.alarm.entity.Alarm">
        select a.* from alarm a where a.type= 5 and a.bind_code = #{bindcode}
    </select>

</mapper>